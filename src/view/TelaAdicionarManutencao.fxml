                        <Label text="Vendedor" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                        <Label text="Forma de Pagamento" GridPane.columnIndex="3" GridPane.rowIndex="5" />
                        <Label fx:id="parcelasLabel" text="Parcelas" GridPane.columnIndex="4" GridPane.rowIndex="5" />
                        <Label text="Preço R$" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                     </children>
                  </GridPane>
                  <GridPane hgap="10.0" VBox.vgrow="ALWAYS">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity" prefWidth="150.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="210.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity" prefWidth="100.0" />
                        <ColumnConstraints />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="ALWAYS" />
                      <RowConstraints vgrow="ALWAYS" />
                        <RowConstraints maxHeight="40.0" minHeight="40.0" prefHeight="40.0" vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                      <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <TextArea fx:id="descricaoArea" GridPane.columnSpan="3" GridPane.rowIndex="1" />
                        <TextField fx:id="marcaText" GridPane.rowIndex="4" />
                        <HBox alignment="TOP_CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <children>
                              <Rectangle fx:id="corRectangle" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="35.0" stroke="BLACK" strokeType="INSIDE" width="35.0" />
                              <ColorPicker fx:id="corColorPicker" minHeight="35.0" />
                           </children>
                        </HBox>
                        <TextField fx:id="modeloText" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                        <Label text="Informações do Aparelho Celular" GridPane.columnSpan="4" GridPane.rowIndex="2">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <ComboBox fx:id="estadoComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                        <Label text="Descrição da Manutenção" />
                        <Label text="Marca" GridPane.rowIndex="3" />
                        <Label text="Cor do Celular" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label text="Modelo" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                        <Label text="Estado da Manutenção" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <Label text="IMEI" GridPane.rowIndex="5" />
