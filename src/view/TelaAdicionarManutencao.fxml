                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="-Infinity" prefWidth="160.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <CheckBox fx:id="editarClienteCheckBox" focusTraversable="false" mnemonicParsing="false" text="Editar informações do Cliente" GridPane.columnSpan="3" />
                        <Button alignment="CENTER" mnemonicParsing="false" onAction="#pesquisarCliente" styleClass="button-pesquisar" text="Pesquisar Cliente" GridPane.columnIndex="6" />
                        <TextField fx:id="nomeText" GridPane.columnSpan="6" GridPane.rowIndex="2" />
                        <TextField fx:id="telefoneText" promptText="(XX)XXXXX-XXXX" GridPane.columnIndex="6" GridPane.rowIndex="2">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="cpfText" promptText="XXX.XXX.XXX-XX" GridPane.rowIndex="4" />
                        <TextField fx:id="rgText" promptText="XX.XXX.XXX-XX" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <TextField fx:id="cidadeText" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                        <TextField fx:id="enderecoText" GridPane.columnIndex="3" GridPane.columnSpan="4" GridPane.rowIndex="4" />
                        <DatePicker fx:id="dataDatePicker" GridPane.rowIndex="6" />
                        <TextField fx:id="precoText" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                        <ComboBox fx:id="vendedorComboBox" prefWidth="150.0" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="6" />
                        <ComboBox fx:id="formarPagComboBox" prefWidth="170.0" GridPane.columnIndex="3" GridPane.rowIndex="6" />
                        <Spinner fx:id="parcelasSpinner" prefWidth="100.0" GridPane.columnIndex="4" GridPane.rowIndex="6" />
