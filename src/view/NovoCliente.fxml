               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </VBox>
      <VBox GridPane.rowIndex="3">
         <children>
            <Label text="CPF" />
            <TextField fx:id="cpfText" promptText="XXX.XXX.XXX-XX" />
         </children>
      </VBox>
      <VBox GridPane.columnIndex="1" GridPane.rowIndex="3">
         <children>
            <Label text="RG" />
            <TextField fx:id="rgText" promptText="XX.XXX.XXX-XX" />
         </children>
      </VBox>
      <VBox GridPane.columnIndex="2" GridPane.rowIndex="3">
         <children>
            <Label text="Cidade" />
            <TextField fx:id="cidadeText" />
         </children>
      </VBox>
      <VBox GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="3">
         <children>
            <Label text="EndereÃ§o" />
            <TextField fx:id="enderecoText" />
         </children>
      </VBox>
      <Button mnemonicParsing="false" prefHeight="40.0" text="Selecionar Cliente" GridPane.columnSpan="2" />
   </children>
