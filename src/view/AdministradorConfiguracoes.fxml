         </padding>
      </VBox>
      <VBox fx:id="painelAdministrador" alignment="TOP_RIGHT" layoutX="269.0" layoutY="144.0" prefHeight="450.0" prefWidth="1200.0" visible="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
